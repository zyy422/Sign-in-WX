/* pages/my/my.wxss */
page {
  background-color: #ffffff;
}
.sectionTitle {
  padding-left: 24rpx;
  padding-right: 22rpx;
  position: relative;
  font-size: 32rpx;
  color: #343434;
  font-weight: 700;
  /* border-bottom: 1rpx solid #EEEEEE; */
  line-height: 80rpx;
}

.sectionTitle::after {
  position: absolute;
  left: 0rpx;
  top: 50%;
  transform: translateY(-50%);
  width: 8rpx;
  height: 28rpx;
  content: '';
  background: linear-gradient(180deg, #FF6E6E, #FF3736);
  border-radius: 4rpx;
}

.tabBar {
  width: 100%;
  height: 100rpx;
  position: fixed;
  left: 0;
  bottom: 0;
  box-shadow: 1rpx 3rpx 59rpx 0px rgba(18, 24, 54, 0.13);
}

.tabBar .item {
  height: 100rpx;
  padding-top: 10rpx;
  padding-bottom: 13rpx;
  font-size: 24rpx;
  color: #999;
}

.tabBar .item.select {
  color: #FF403E;
}

.tabBar .item .indexPic {
  width: 33rpx;
  height: 41rpx;
}

.tabBar .item .myPic {
  width: 33rpx;
  height: 32rpx;
}

.tabBar .item .select {
  color: #FF403E;
}

.tabBar .tab3 {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  position: absolute;
  left: 50%;
  top: -50rpx;
  transform: translateX(-50%);
}

.myView .topBox {
  padding-top: 6vh;
  background-color: #ebd5c8;
  /* min-height: 380rpx; */
}

.myView .topBox .userInfo {
  font-weight: 500;
}

.myView .topBox .userInfo .name {
  font-size: 32rpx;
  font-weight: 500;
  color: #343434;
  margin-right: 10rpx;
  margin-left: 23rpx;
}

.myView .topBox .userInfo>image {
  width: 74rpx;
  height: 74rpx;
  border-radius: 50%;
  margin-left: 62rpx;
}

.myView .topBox .card {
  position: relative;
  margin: 0 22rpx;
  margin-top: 33rpx; 
  height: 214rpx;
}

.myView .topBox .card .bg {
  position: absolute;
  left: 0;
  top: 0;
  border-radius: 16rpx;
  width: 100%;
  box-shadow: 1rpx 3rpx 59rpx 0px rgba(18, 24, 54, 0.8);
}

.myView .topBox .cardDesc {
  width: 100%;
  position: absolute;
  left: 24rpx;
  top: 24rpx;
  color: rgba(210, 182, 144, .53);
  font-size: 28rpx;
  padding-right: 48rpx;
}

.myView .topBox .cardDesc button {
  margin-right: 24rpx;
  padding: 0;
  width: 133rpx;
  height: 44rpx;
  line-height: 44rpx;
  background: #CEA882;
  border-radius: 22rpx;
  font-size: 26rpx;
  color: #fff;
  margin: 0;
}

.myView .radianBox {
  position: relative;
  top: -100rpx;
  height: 140rpx;
}

.myView .radianBox .bg {
  width: 100%;
  position: absolute;
  left: 0;
  top: 0;
}

.myView  .shadowBox {
  width: calc(100%  - 44rpx); 
  box-shadow: 1rpx 3rpx 13rpx 0px rgba(35, 35, 35, 0.14);
  border-radius: 12rpx;
  margin-left: 22rpx;
}
.myView .radianBox .shadowBox.s1{
  position: absolute;
  top: 102rpx;
  padding-top: 25rpx;
  padding-bottom: 33rpx;
}
.myView .radianBox .shadowBox.s1 .item{
  flex: 1;
}
.myView .radianBox .shadowBox.s1 .item:first-child{
  border-right: 1rpx solid #E5E5E5;
}
.myView .radianBox .shadowBox.s1 .item image {
  width: 42rpx;
  height: 46rpx;
  margin-right: 20rpx;
}

.myView .radianBox .shadowBox.s1 .item .w46 {
  width: 46rpx;
  height: 44rpx;
}

.myView .radianBox .shadowBox.s1 .item .name {
  font-size: 30rpx;
  font-weight: 400;
  color: #343434;
}

.myView  .shadowBox.s2 {
  padding:0 22rpx ;
}
.myView  .shadowBox.s2 .sectionTitle{
  border-bottom: 1rpx solid #EEEEEE;
}
.myView  .shadowBox.s2  .item{
font-size: 26rpx;
font-weight: 400;
color: #727272;
line-height: 34rpx;
flex: 1;
box-sizing: content-box;
height: 78rpx;
padding-top: 30rpx;
padding-bottom: 27rpx;
}
.myView  .shadowBox.s2  .item image{
  width: 36rpx; 
}
#userLogin{
  height: 50;
}
.login-btn{
  background-color:#ebd5c8;
  width: 116;
  display: flex;
  justify-content: flex-start;
  margin: 0 0 0 0;
  padding: 0 0 0 0;
}
.login-btn::after{
  border: 0; 
}